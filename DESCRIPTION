Package: creelutils
Title: Utility Functions for Interacting with Freshwater Creel Data
Version: 0.1.0
Authors@R: c(
    person("Colt", "Holley", email = "colt.holley@dfw.wa.gov", role = c("aut", "cre"), comment = c(ORCID = "0000-0003-4172-4331")),
